{
  "Serilog": {
	"MinimumLevel": {
		"Default": "Information",
		"Override": {
			"Microsoft": "Warning",
			"Microsoft.Hosting.Lifetime": "Information"
		}
	},
	"WriteTo": [
		{
			"Name": "File",
			"Args": {
				"path": "./logs/log-.txt",
				"rollingInterval": "Day"
			}
		},
		{
			"Name": "Seq",
			"Args": { "serverUrl": "http://localhost:15252" } 
		}
	] 
  },
  "AllowedHosts": "*"
}
